            <Rectangle Stroke="#FF000000" Fill="#CA5C5A5A" x:Name="status_rect_fader" Visibility="Collapsed"/>
            <Rectangle x:Name="status_rect_fader2" Stroke="#FF000000" Visibility="Collapsed" Fill="#E0000000" Margin="-51,-23,-19,-29"/>
